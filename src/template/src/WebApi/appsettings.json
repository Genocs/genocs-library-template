{
  "app": {
    "name": "Genocs.Library.Template WebApi",
    "service": "genocs.library.template-service",
    "instance": "1",
    "version": "v1.0",
    "displayBanner": true,
    "displayVersion": true
  },
  "consul": {
    "enabled": false,
    "url": "http://localhost:8500",
    "service": "genocs.library.template-service",
    "address": "docker.for.win.localhost",
    "port": "5002",
    "pingEnabled": true,
    "pingEndpoint": "ping",
    "pingInterval": 3,
    "removeAfterInterval": 3
  },
  "fabio": {
    "enabled": false,
    "url": "http://localhost:9999",
    "service": "genocs.library.template-service"
  },
  "httpClient": {
    "type": "",
    "retries": 2,
    "services": {}
  },
  "logger": {
    "level": "debug",
    "applicationName": "genocs.library.template-service",

    "excludePaths": [
      "/",
      "/healthz",
      "/alive",
      "/metrics"
    ],
    "excludeProperties": [
      "api_key",
      "access_key",
      "ApiKey",
      "ApiSecret",
      "ClientId",
      "ClientSecret",
      "ConnectionString",
      "Password",
      "Email",
      "Login",
      "Secret",
      "Token"
    ],
    "console": {
      "enabled": true,
      "enableTracing": true
    },
    "file": {
      "enabled": true,
      "path": "logs/logs.txt",
      "interval": "day"
    },
    "seq": {
      "enabled": false,
      "url": "http://localhost:5341",
      "token": "secret"
    }
  },
  "jaeger": {
    "enabled": false,
    "serviceName": "genocs.library.template webapi",
    "udpHost": "localhost",
    "udpPort": 6831,
    "maxPacketSize": 0,
    "sampler": "const",
    "excludePaths": [
      "/",
      "/ping",
      "/metrics"
    ]
  },
  "metrics": {
    "enabled": true,
    "influxEnabled": false,
    "prometheusEnabled": false,
    "influxUrl": "http://localhost:8086",
    "database": "test",
    "env": "local",
    "interval": 5
  },
  "mongoDb": {
    "connectionString": "mongodb://localhost:27017",
    "database": "genocslibrarytemplate-service",
    "enableTracing": true,
    "seed": false
  },
  "prometheus": {
    "enabled": false,
    "endpoint": "http://localhost:5071"
  },
  "rabbitMq": {
    "connectionName": "genocs.library.template-service",
    "messagesPersisted": true,
    "retries": 3,
    "retryInterval": 2,
    "conventionsCasing": "snakeCase",
    "logger": {
      "enabled": true,
      "logConnectionStatus": true,
      "logMessagePayload": true
    },
    "username": "guest",
    "password": "guest",
    "virtualHost": "/",
    "port": 5672,
    "hostName": "localhost",
    "hostnames": [
      "localhost",
      "rabbitmq"
    ],
    "requestedConnectionTimeout": "00:00:30",
    "requestedHeartbeat": "00:01:00",
    "socketReadTimeout": "00:00:30",
    "socketWriteTimeout": "00:00:30",
    "continuationTimeout": "00:00:20",
    "handshakeContinuationTimeout": "00:00:10",
    "networkRecoveryInterval": "00:00:05",
    "exchange": {
      "declare": true,
      "durable": true,
      "autoDelete": false,
      "type": "topic",
      "name": "genocs.library.template"
    },
    "queue": {
      "declare": true,
      "durable": true,
      "exclusive": false,
      "autoDelete": false,
      "template": "{{assembly}}/{{exchange}}.{{message}}"
    },
    "context": {
      "enabled": true,
      "header": "message_context"
    },
    "spanContextHeader": "span_context",
    "deadLetter": {
      "enabled": true,
      "prefix": "dlx-",
      "declare": true
    },
    "maxProducerChannels": 1000
  },
  "swagger": {
    "enabled": true,
    "reDocEnabled": true,
    "name": "Genocs.Library.Template",
    "title": "Genocs.Library.Template WebApi",
    "version": "v2",
    "description": "The Genocs.Library.Template API.",
    "routePrefix": "swagger",
    "contactName": "Giovanni Nocco",
    "contactEmail": "giovanni.nocco@genocs.com",
    "contactUrl": "https://www.genocs.com",
    "licenseName": "Use under MIT",
    "licenseUrl": "https://opensource.org/license/mit",
    "termsOfService": "https://www.genocs.com/terms_and_conditions.html",
    "includeSecurity": true,
    "servers": [
      {
        "url": "http://localhost:5071",
        "description": "Local version to be used for development"
      }
    ]
  },
  "redis": {
    "connectionString": "localhost",
    "instance": "genocs.library.template:"
  },
  "security": {
    "certificate": {
      "enabled": false,
      "header": "Certificate",
      "allowedHosts": [
        "localhost",
        "product"
      ]
    }
  },
  "vault": {
    "enabled": false,
    "url": "http://localhost:8200",
    "authType": "token",
    "token": "secret",
    "username": "user",
    "password": "secret",
    "kv": {
      "enabled": false,
      "engineVersion": 2,
      "mountPoint": "kv",
      "path": "template-service/settings"
    },
    "pki": {
      "enabled": false,
      "roleName": "template-service",
      "commonName": "genocs.library.template.demo.io"
    },
    "lease": {
      "mongo": {
        "type": "database",
        "roleName": "template-service",
        "enabled": false,
        "autoRenewal": true,
        "templates": {
          "connectionString": "mongodb://{{username}}:{{password}}@localhost:27017"
        }
      }
    }
  },
  "AllowedHosts": "*"
}