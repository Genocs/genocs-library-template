{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information",
      "Microsoft": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "ApplicationInsights": ""
  },
  "AppSettings": {
    "ServiceName": "Demo-WebApi"
  },
  "rabbitMq": {
    "connectionName": "users-service",
    "retries": 3,
    "retryInterval": 2,
    "conventionsCasing": "snakeCase",
    "logger": {
      "enabled": true
    },
    "username": "guest",
    "password": "guest",
    "virtualHost": "/",
    "port": 5672,
    "HostName": "localhost",
    "hostnames": [
      "localhost"
    ],
    "requestedConnectionTimeout": "00:00:30",
    "requestedHeartbeat": "00:01:00",
    "socketReadTimeout": "00:00:30",
    "socketWriteTimeout": "00:00:30",
    "continuationTimeout": "00:00:20",
    "handshakeContinuationTimeout": "00:00:10",
    "networkRecoveryInterval": "00:00:05",
    "exchange": {
      "declare": true,
      "durable": true,
      "autoDelete": false,
      "type": "topic",
      "name": "Genocs.WebApiTemplate"
    },
    "queue": {
      "declare": true,
      "durable": true,
      "exclusive": false,
      "autoDelete": false,
      "template": "Genocs.WebApiTemplate-service/{{exchange}}.{{message}}"
    },
    "context": {
      "enabled": true,
      "header": "message_context"
    },
    "spanContextHeader": "span_context"
  },
  "mongoDb": {
    "connectionString": "mongodb://localhost:27017",
    "database": "orders-service",
    "enableTracing": "true",
    "seed": false
  },
  "mongo": {
    "connectionString": "mongodb://localhost:27017",
    "database": "orders-service",
    "enableTracing": "true",
    "seed": false
  },
  "JWT": {
    "ValidIssuer": "http://localhost/Auth",
    "ValidAudience": "https://localhost:5000",
    "Secret": "<<>>"
  },
  "Monitoring": {
    "Jaeger": "localhost"
  }
}
