{
  "app": {
    "name": "Genocs.WebApiTemplate Worker",
    "service": "template-worker",
    "instance": "000001",
    "version": "v1.0",
    "displayBanner": true,
    "displayVersion": true
  },
  "consul": {
    "enabled": false,
    "url": "http://localhost:8500",
    "service": "template-worker",
    "address": "docker.for.win.localhost",
    "port": "5002",
    "pingEnabled": true,
    "pingEndpoint": "ping",
    "pingInterval": 3,
    "removeAfterInterval": 3
  },
  "fabio": {
    "enabled": false,
    "url": "http://localhost:9999",
    "service": "template-worker"
  },
  "httpClient": {
    "type": "fabio",
    "retries": 3,
    "services": {},
    "requestMasking": {
      "enabled": true,
      "maskTemplate": "*****"
    },
    "correlationIdHeader": "x-correlation-id"
  },
  "logger": {
    "applicationName": "template-worker",
    "level": "information",
    "excludePaths": [
      "/",
      "/ping",
      "/metrics"
    ],
    "excludeProperties": [
      "api_key",
      "access_key",
      "ApiKey",
      "ApiSecret",
      "ClientId",
      "ClientSecret",
      "ConnectionString",
      "Password",
      "Email",
      "Login",
      "Secret",
      "Token"
    ],
    "console": {
      "enabled": true
    },
    "file": {
      "enabled": true,
      "path": "logs/logs.txt",
      "interval": "day"
    },
    "seq": {
      "enabled": false,
      "url": "http://localhost:5341",
      "token": "secret"
    }
  },
  "jaeger": {
    "enabled": true,
    "serviceName": "templateworker",
    "udpHost": "localhost",
    "udpPort": 6831,
    "maxPacketSize": 0,
    "sampler": "const",
    "excludePaths": [
      "/ping",
      "/metrics"
    ]
  },
  "metrics": {
    "enabled": true,
    "influxEnabled": false,
    "prometheusEnabled": false,
    "influxUrl": "http://localhost:8086",
    "database": "test",
    "env": "local",
    "interval": 5
  },
  "mongo": {
    "connectionString": "mongodb://localhost:27017",
    "database": "template-service",
    "seed": false,
    "enableTracing": true
  },
  "mongoDb": {
    "connectionString": "mongodb://localhost:27017",
    "database": "template-service",
    "seed": false
  },
  "prometheus": {
    "enabled": false
  },
  "rabbitMq": {
    "connectionName": "template-service",
    "messagesPersisted": true,
    "retries": 3,
    "retryInterval": 2,
    "conventionsCasing": "snakeCase",
    "logger": {
      "enabled": true,
      "logConnectionStatus": true,
      "logMessagePayload": true
    },
    "username": "guest",
    "password": "guest",
    "virtualHost": "/",
    "port": 5672,
    "hostName": "localhost",
    "hostnames": [
      "localhost",
      "rabbitmq"
    ],
    "requestedConnectionTimeout": "00:00:30",
    "requestedHeartbeat": "00:01:00",
    "socketReadTimeout": "00:00:30",
    "socketWriteTimeout": "00:00:30",
    "continuationTimeout": "00:00:20",
    "handshakeContinuationTimeout": "00:00:10",
    "networkRecoveryInterval": "00:00:05",
    "exchange": {
      "declare": true,
      "durable": true,
      "autoDelete": false,
      "type": "topic",
      "name": "template"
    },
    "queue": {
      "declare": true,
      "durable": true,
      "exclusive": false,
      "autoDelete": false,
      "template": "{{assembly}}/{{exchange}}.{{message}}"
    },
    "context": {
      "enabled": true,
      "header": "message_context"
    },
    "spanContextHeader": "span_context",
    "deadLetter": {
      "enabled": true,
      "prefix": "dlx-",
      "declare": true
    },
    "maxProducerChannels": 1000
  },
  "swagger": {
    "enabled": true,
    "reDocEnabled": false,
    "name": "v1",
    "title": "Genocs.WebApiTemplate",
    "version": "v1",
    "routePrefix": "swagger"
  },
  "redis": {
    "connectionString": "localhost",
    "instance": "template:"
  },
  "security": {
    "certificate": {
      "enabled": false,
      "header": "Certificate",
      "allowedHosts": [
        "localhost"
      ]
    }
  },
  "vault": {
    "enabled": false,
    "url": "http://localhost:8200",
    "authType": "token",
    "token": "secret",
    "username": "user",
    "password": "secret",
    "kv": {
      "enabled": false,
      "engineVersion": 2,
      "mountPoint": "kv",
      "path": "template-service/settings"
    },
    "pki": {
      "enabled": false,
      "roleName": "template-service",
      "commonName": "template-service.demo.io"
    },
    "lease": {
      "mongo": {
        "type": "database",
        "roleName": "template-service",
        "enabled": false,
        "autoRenewal": true,
        "templates": {
          "connectionString": "mongodb://{{username}}:{{password}}@localhost:27017"
        }
      }
    }
  },
  "webApi": {
    "bindRequestFromRoute": true
  }
}