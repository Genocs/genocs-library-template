{
  "$schema": "http://json.schemastore.org/template",
  "author": "Nocco Giovanni Emanuele",
  "classifications": [
    "Web/ASP.NET"
  ],
  "name": "Genocs: Web Api .NET7",
  "identity": "Genocs.WebApiTemplate",
  "groupIdentity": "Genocs.WebApiTemplate",
  "shortName": "gnx-webapi",
  "tags": {
    "type": "project",
    "language": "C#" // Specify that this template is in C#.
  },
  "sourceName": "Genocs.WebApiTemplate", // Will replace the string 'Genocs.WebApiTemplate' with the value provided via -n.
  "preferNameDirectory": true,
  "symbols": {
    "genocs-components": {
      "type": "parameter",
      "datatype": "choice",
      "defaultValue": "full",
      "choices": [
        {
          "choice": "full",
          "description": "Setup the full set of Genocs Library functionalities"
        }
      ],
      "replaces": "genocs-components",
      "onlyIf": [
        {
          "after": "\"genocs-components\": \""
        }
      ]
    },
    "full": {
      "type": "computed",
      "value": "(genocs-components == \"full\")"
    }
  },
  "sources": [
    {
      "source": "./",
      "modifiers": [
        {
          "condition": "(Full)",
          "exclude": []
        }
      ]
    }
  ],
  "primaryOutputs": [
    {
      "path": "src/Genocs.WebApiTemplate.AcceptanceTests/Genocs.WebApiTemplate.AcceptanceTests.csproj"
    },
    {
      "path": "src/Genocs.WebApiTemplate.Contracts/Genocs.WebApiTemplate.Contracts.csproj"
    },
    {
      "path": "src/Genocs.WebApiTemplate.Domain/Genocs.WebApiTemplate.Domain.csproj"
    },
    {
      "path": "src/Genocs.WebApiTemplate.Infrastructure/Genocs.WebApiTemplate.Infrastructure.csproj"
    },
    {
      "path": "src/Genocs.WebApiTemplate.IntegrationTests/Genocs.WebApiTemplate.IntegrationTests.csproj"
    },
    {
      "path": "src/Genocs.WebApiTemplate.UnitTests/Genocs.WebApiTemplate.UnitTests.csproj"
    },
    {
      "path": "src/Genocs.WebApiTemplate.WebApi/Genocs.WebApiTemplate.WebApi.csproj"
    },
    {
      "path": "src/Genocs.WebApiTemplate.Worker/Genocs.WebApiTemplate.Worker.csproj"
    }
  ]
}