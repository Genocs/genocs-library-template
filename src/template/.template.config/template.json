{
  "$schema": "http://json.schemastore.org/template",
  "author": "Nocco Giovanni Emanuele",
  "classifications": [
    "Web/ASP.NET"
  ],
  "name": "Genocs: Web Api .NET7",
  "identity": "Genocs.MicroserviceTemplate",
  "groupIdentity": "Genocs.MicroserviceTemplate",
  "shortName": "webapi",
  "tags": {
    "language": "C#" // Specify that this template is in C#.
  },
  "sourceName": "Genocs.MicroserviceTemplate", // Will replace the string 'Genocs.MicroserviceTemplate' with the value provided via -n.
  "preferNameDirectory": true,
  "symbols": {
    "use-cases": {
      "type": "parameter",
      "datatype": "choice",
      "defaultValue": "full",
      "choices": [
        {
          "choice": "full",
          "description": "Full Wallet Account Balance Management (Read/Write)"
        }
      ],
      "replaces": "use-cases",
      "onlyIf": [
        {
          "after": "\"use-cases\": \""
        }
      ]
    },
    "full": {
      "type": "computed",
      "value": "(use-cases == \"full\")"
    }
  },
  "sources": [
    {
      "source": "./",
      "modifiers": [
        {
          "condition": "(Full)",
          "exclude": []
        }
      ]
    }
  ],
  "primaryOutputs": [
    {
      "path": "src/Genocs.MicroserviceTemplate.AcceptanceTests/Genocs.MicroserviceTemplate.AcceptanceTests.csproj"
    },
    {
      "path": "src/Genocs.MicroserviceTemplate.Application/Genocs.MicroserviceTemplate.Application.csproj"
    },
    {
      "path": "src/Genocs.MicroserviceTemplate.Domain/Genocs.MicroserviceTemplate.Domain.csproj"
    },
    {
      "path": "src/Genocs.MicroserviceTemplate.Infrastructure/Genocs.MicroserviceTemplate.Infrastructure.csproj"
    },
    {
      "path": "src/Genocs.MicroserviceTemplate.IntegrationTests/Genocs.MicroserviceTemplate.IntegrationTests.csproj"
    },
    {
      "path": "src/Genocs.MicroserviceTemplate.Shared/Genocs.MicroserviceTemplate.Shared.csproj"
    },
    {
      "path": "src/Genocs.MicroserviceTemplate.UnitTests/Genocs.MicroserviceTemplate.UnitTests.csproj"
    },
    {
      "path": "src/Genocs.MicroserviceTemplate.WebApi/Genocs.MicroserviceTemplate.WebApi.csproj"
    },
    {
      "path": "src/Genocs.MicroserviceTemplate.Worker/Genocs.MicroserviceTemplate.Worker.csproj"
    },
    {
      "path": "src/Genocs.MicroserviceTemplate.Worker.MassTransit/Genocs.MicroserviceTemplate.Worker.MassTransit.csproj"
    }
  ]
}